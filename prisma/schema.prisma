generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int @id @default(autoincrement())  
  uname     String 
  email     String   
  phone     String
  birthday  String
  time      String
  location  String
  questions String
  rtype String
  price String
}
